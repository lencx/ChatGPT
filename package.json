    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "sass": "^1.56.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
